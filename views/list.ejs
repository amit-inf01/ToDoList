
<%- include('header'); -%>

    <div class="box" id="heading">
     <h1><%= dayName %></h1>
    </div>
    <div class="box" >

      <% newItem.forEach(function(element){ %>
        <div class="item">
          <form class="" action="/" method="post">
            <input type="checkbox" name="check" value=<%=element._id%> onChange="this.form.submit()">
            <p><%= element.name %></p>
            <input type="hidden" name="list" value="<%= dayName %>">
          </form>
        </div>
      <%})%>


    <form class="amit" action="/" method="post" >
      <input type="text" name="what" value="" placeholder="New Item" autocomplete="off">
      <button type="submit" name="button" value=<%= wButton %> > + </button>
    </form>
    </div>

    <%- include('footer'); -%>
